CÃ³digo para estudo:

#include <stdio.h>
#include <stdlib.h>
#include <time.h>
#include <limits.h>
#include <math.h>

int analisaMaior1(const void * a, const void * b) {
  return ( *(int*)a - *(int*)b );
}

void analisaMaior2(float *numb, int n) {
    int i, j;

    float tmp = 0;

    for(i = 0; i < n; i++) {
        for(j = i + 1; j < n; j++) {
            if(numb[i] > numb[j]) {
                tmp = numb[i];
                numb[i] = numb[j];
                numb[j] = tmp;
            }
        }

    }

    for(i = 0; i < n; i++) {
        printf("%.2f ", numb[i]);
    }
    printf("\n");

}

int main() {

    clock_t t1, t2;
    int i, j;

    t1 = clock();
    for(j=0; j<5; j++){
        int n, i;

        float *x;

        printf("Digite o tamanho: ");
        scanf("%d", &n);

        x = (float*) malloc(n*sizeof(float));

        for(i = 0; i < n; i++) {
            scanf("%f" , &x[i]);
        }

        qsort(x, n, sizeof(float), analisaMaior1);

        for(i = 0; i < n; i++) {
            printf("%.2f " , x[i]);
        }
        free(x);
    }

    t2 = clock()-t1;
    printf ("%d clicks (%f s).\n", t2,((float)t2)/CLOCKS_PER_SEC);

    t1 = clock();
    for(i=0; i<5; i++){
            
        int n, i;

        float *x;

        printf("Digite o tamanho: ");
        scanf("%d", &n);

        x = (float*) malloc(n*sizeof(float));

        for(i = 0; i < n; i++) {
            scanf("%f" , &x[i]);
        }

        analisaMaior2(x, n);

        free(x);
    }
    t2 = clock()-t1;
    printf("%d clicks (%f s).\n", t2,((float)t2)/CLOCKS_PER_SEC);





    return 0;
}
