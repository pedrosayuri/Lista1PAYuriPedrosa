#include <stdio.h>
#include <stdlib.h>

int analisaMaior (const void * a, const void * b) {
  return ( *(int*)a - *(int*)b );
}

int main() {

    int n, i;

    float *x;

    printf("Digite o tamanho: ");
    scanf("%d", &n);

    x = (float*) malloc(n*sizeof(float));

    for(i = 0; i < n; i++) {
        scanf("%f" , &x[i]);
    }

    qsort(x, n, sizeof(float), analisaMaior);

    for(i = 0; i < n; i++) {
        printf("%.2f " , x[i]);
    }

    free(x);

    return 0;
}
